# Configurações do The Wheel Screener
# Copie este arquivo para .env e configure suas variáveis

# Token da API OpLab (opcional - para dados reais)
OPLAB_API_TOKEN=

# Configurações do Flask
FLASK_ENV=development
FLASK_DEBUG=1

# URL da API para o frontend
VITE_API_URL=http://localhost:5000

# Configurações do banco de dados (opcional)
DATABASE_URL=sqlite:///backend-oplab/src/database/app.db
